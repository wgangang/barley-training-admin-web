{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@data": [
        "./src/data.ts"
      ],
      "@core/*": [
        "./src/core/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@apis/*": [
        "./src/apis/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
